#include<stdio.h>
int sum(int a,int b);
int sub(int a,int b);
int div(int a,int b);
int mul(int a,int b);
int main()
{
	int n,m;
	scanf("%d %d",&n,&m);
	int res=sum(n,m);
	int ans=sub(n,m);
	int j=div(n,m);
	int u=mul(n,m);
	printf("SUM = %d\n SUB = %d\n DIV= %d\n MUL=%d\n",res,ans,j,u);
}
int sum(int a,int b)
{
	int s=a+b;
	return s;
}
int sub(int a,int b){
	int p=a-b;
	return p;
}
int div(int a,int b)
{
	int d=a/b;
	return d;
}
int mul(int a,int b){
	int ml=a*b;
	return ml;
}
